{
  "name": "angular-docker",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "docker:build:dev": "docker build -t angular-docker:dev --build-arg configuration='' .",
    "docker:build:prod": "docker build -t angular-docker:prod --build-arg configuration='production' .",
    "docker:run:dev": "docker run -p 80:80 angular-docker:dev",
    "docker:run:prod": "docker run -p 80:80 angular-docker:prod",
    "docker:dev": "npm run docker:build:dev && npm run docker:run:dev",
    "docker:prod": "npm run docker:build:prod && npm run docker:run:prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.3",
    "@angular/common": "~10.1.3",
    "@angular/compiler": "~10.1.3",
    "@angular/core": "~10.1.3",
    "@angular/forms": "~10.1.3",
    "@angular/platform-browser": "~10.1.3",
    "@angular/platform-browser-dynamic": "~10.1.3",
    "@angular/router": "~10.1.3",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.3",
    "@angular/cli": "~10.1.3",
    "@angular/compiler-cli": "~10.1.3",
    "@types/node": "^12.11.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
